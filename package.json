{
  "name": "wanderlist",
  "version": "0.1.0",
  "description": "Wanderlist ----",
  "main": "index.js",
  "scripts": {
    "tiye": "rsync -r dist/ repo.tiye.me:repo/Memkits/wanderlist --delete",
    "cljs-release": "shadow-cljs -b release --release",
    "cljs-once": "shadow-cljs -b npm --once",
    "cljs-browser": "shadow-cljs -b dev --dev",
    "cljs": "shadow-cljs -b npm --dev",
    "html-dev": "env=dev node target/app.render.js",
    "html": "node target/app.render.js",
    "hmr": "webpack-dev-server --config webpack.dev.coffee --hot-only",
    "webpack": "webpack --hide-modules --progress",
    "dev": "yarn cljs-once && yarn html-dev && yarn hmr",
    "del": "rm -rf dist/*",
    "serve": "http-server dist/",
    "build": "yarn del && yarn cljs-release && yarn webpack && yarn cljs-once && yarn html && yarn serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Memkits/wanderlist.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Memkits/wanderlist/issues"
  },
  "homepage": "https://github.com/Memkits/wanderlist#readme",
  "devDependencies": {
    "coffee-script": "^1.12.6",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "http-server": "^0.10.0",
    "ionicons": "^3.0.0",
    "shadow-cljs": "^0.9.8",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.9",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.4.5",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "dependencies": {}
}
